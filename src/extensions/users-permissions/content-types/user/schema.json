{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "majlis": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::majlis.majlis",
      "inversedBy": "users"
    },
    "user_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::user-type.user-type",
      "inversedBy": "users_permissions_users"
    },
    "name": {
      "type": "string"
    },
    "surname": {
      "type": "string"
    },
    "dzemat": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::dzemat.dzemat",
      "inversedBy": "users"
    },
    "muftijstvo": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::muftijstvo.muftijstvo",
      "inversedBy": "users"
    },
    "data_dzemat": {
      "type": "json"
    },
    "data_majlis": {
      "type": "json"
    },
    "data_muftijstvo": {
      "type": "json"
    }
  }
}
